{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TopoJSON Object",
  "type": "object",
  "required": ["type"],
  "properties": {
    "type": {
      "enum": [
        "Topology",
        "Point",
        "MultiPoint",
        "LineString",
        "MultiLineString",
        "Polygon",
        "MultiPolygon",
        "GeometryCollection"
      ]
    },
    "bbox": {
      "$ref": "bbox.json"
    }
  }
}
